{
    "name": "mhmadahmd/filasaas",
    "description": "This is my package filasaas",
    "keywords": [
        "mhmadahmd",
        "laravel",
        "filasaas"
    ],
    "homepage": "https://github.com/mhmadahmd/filasaas",
    "support": {
        "issues": "https://github.com/mhmadahmd/filasaas/issues",
        "source": "https://github.com/mhmadahmd/filasaas"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Mohammad Ahmad",
            "email": "mhmad-ahmd@outlook.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/filament": "^4.0",
        "laravel/framework": "12.*",
        "nesbot/carbon": "3.*",
        "orchestra/testbench": "^10.4",
        "spatie/eloquent-sortable": "^4.0",
        "spatie/laravel-package-tools": "^1.15.0",
        "spatie/laravel-sluggable": "^3.0",
        "spatie/laravel-translatable": "^6.0",
        "srmklive/paypal": "~3.0"
    },
    "suggest": {
        "laravel/cashier-stripe": "^15.0 for Stripe payment gateway support"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "pestphp/pest": "^3.7",
        "pestphp/pest-plugin-laravel": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "Mhmadahmd\\Filasaas\\": "src/",
            "Mhmadahmd\\Filasaas\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mhmadahmd\\Filasaas\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mhmadahmd\\Filasaas\\FilasaasServiceProvider"
            ],
            "aliases": {
                "Filasaas": "Mhmadahmd\\Filasaas\\Facades\\Filasaas"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
